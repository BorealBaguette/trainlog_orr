{
  "distance_influence": 0,
  "priority": [
    {
      "if": "railway_service == YARD",
      "multiply_by": "0.01"
    },
    {
      "else_if": "!rail_access",
      "multiply_by": "0.01"
    },
    {
      "else_if": "rail_access == true",
      "multiply_by": "1.0"
    },
    {
      "else": "",
      "multiply_by": "0.5"
    }
  ],
  "speed": [
    {
      "if": "railway_service == SIDING || railway_service == SPUR || railway_service == YARD",
      "limit_to": "30"
    },
    {
      "else_if": "max_speed > 0",
      "limit_to": "max_speed"
    },
    {
      "else": "",
      "limit_to": "rail_average_speed"
    }
  ]
}